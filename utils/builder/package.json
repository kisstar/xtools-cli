{
  "name": "@xtools-cli/builder",
  "version": "1.0.0",
  "description": "Unified packaging tool for xtools.",
  "type": "module",
  "module": "./dist/lib.es.js",
  "types": "./dist/lib/index.d.ts",
  "main": "./dist/lib.cjs.js",
  "exports": {
    ".": {
      "types": "./dist/lib/index.d.ts",
      "import": "./dist/lib.es.js",
      "require": "./dist/lib.cjs.js"
    }
  },
  "scripts": {
    "dev": "rollup -cw",
    "build": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kisstar/xtools-cli.git",
    "directory": "utils/builder"
  },
  "keywords": [
    "xtools",
    "builder"
  ],
  "author": "kisstar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kisstar/xtools-cli/issues"
  },
  "homepage": "https://github.com/kisstar/xtools-cli#readme",
  "dependencies": {
    "chalk": "^5.0.1",
    "leven": "^4.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "rollup": "^2.76.0"
  }
}
