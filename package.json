  "devDependencies": {
    "@types/node": "^18.0.5",
    "gh-pages": "^4.0.0",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "unbuild": "^0.7.6",
    "vitepress": "1.0.0-alpha.33",
    "vue": "^3.2.45"
